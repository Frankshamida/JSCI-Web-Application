{
  "version": 2,
  "name": "joyful-sound-church",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/entry-point.html"
    },
    {
      "src": "/index",
      "dest": "/index.html"
    },
    {
      "src": "/Points_Giver",
      "dest": "/Points_Giver.html"
    },
    {
      "src": "/Registration",
      "dest": "/Registration.html"
    },
    {
      "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/entry-point.html"
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}